#include <iostream>

using namespace std;

// Function to calculate factorial
long long factorial(int num) {
    long long fact = 1;
    for (int i = 1; i <= num; i++) {
        fact *= i;
    }
    return fact;
}

int main() {
    int n;
    cout << "Enter a number: ";
    cin >> n;

    cout << "Factorials of first " << n << " numbers:\n";
    for (int i = 1; i <= n; i++) {
        cout << "Factorial of " << i << " = " << factorial(i) << endl;
    }

    return 0;
}
